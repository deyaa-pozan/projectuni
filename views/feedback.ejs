<%- include('partials/header'); -%>



    <!-- ORDER DATA-->
    <div class="main-content">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <div class="row">
                    <h4>Feedback Table</h4>
                    <div class="col-lg-12" style="overflow-x:auto;">
                        <table id="table_feedback" class="table table-bordred table-striped" style="overflow-x:auto; ">
                            <thead>
                                <th>Name</th>
                                <th>Email</th>
                                <th>Message</th>
                                <th>Delete</th>
                               
                            </thead>
                            <tbody>

                                <% for (var i=0; i < countfeedback; i++ ) { %>
                                    <tr id="<%=foundfeedback[i]._id%>">
                                        <td>
                                            <%=foundfeedback[i].name%>
                                        </td>
                                        <td>
                                            <%=foundfeedback[i].email%>
                                        </td>
                                       
                                        <td>
                                            <%=foundfeedback[i].message%>
                                        </td>

                                        <td>
                                            <p data-placement="top" data-toggle="tooltip" title="Delete"><a
                                                    href="/deletefeedback/<%=foundfeedback[i]._id %>"><button
                                                        class="fa fa-trash" data-title="Delete" data-toggle="modal"
                                                        data-id="<%=foundfeedback[i]._id %>" id="deletefeedback"><span
                                                            class="glyphicon glyphicon-trash"></span></button></p>
                                        </td>

                                        <% } %>

                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
    </div>

    

    </div>

    <%- include('partials/footer'); -%>