    <style type="text/css">
        /* force class color to override the bootstrap base rule
           NOTE: adding 'url: #' to calendar makes this unneeded
         */
        .fc-event,
        .fc-event:hover {
            color: #fff !important;
            text-decoration: none;
        }
    </style>

</head>

<!-- animsition overrides all click events on clickable things like a,
      since calendar doesn't add href's be default,
      it leads to odd behaviors like loading 'undefined'
      moving the class to menus lead to only the menu having the effect -->

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <div class="logo  rounded text-center ">
                            <a href="/dashbord" class="ml-5">
                                <h4 ><span class="h2 text-success">Scan</span>Market</h4>
                            </a>
                        </div>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">
                        <li class="has-sub">
                            <a class="js-arrow" href="/dashbord">
                                <i class="fas fa-tachometer-alt "></i>Dashboard</a>

                        </li>
                        <li>
                            <a href="/addproduct">
                                <i class="fas fa-plus"></i>Add Item</a>
                        </li>
                        <li>
                            <a href="/order">
                                <i class="fas fa-shapes"></i>Order</a>
                        </li>
                        <li>
                            <a href="/userdata">
                                <i class="fas fa-user"></i>Users</a>
                        </li>
                        <li>
                            <a href="/">
                                <i class="fas fa-percent"></i>Discount</a>
                        </li>
                        <%if(adminpage.__v===1){%>
                            <li>
                                <a href="/register">
                                    <i class="fa fa-registered"></i>register</a>
                            </li>
                            <li>
                                <a href="/adminpage">
                                    <i class="fa fa-id-card"></i>Admin page</a>
                            </li>
                            <%}%>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->

        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo  rounded text-center ">
                <a href="/dashbord" class="ml-5">
                    <h4 ><span class="h2 text-success">Scan</span>Market</h4>
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li class="has-sub">
                            <a class="js-arrow" href="/dashbord">
                                <i class="fas fa-tachometer-alt "></i>Dashboard</a>

                        </li>
                        <li>
                            <a href="/addproduct">
                                <i class="fas fa-plus"></i>Add Item</a>
                        </li>
                        <li>
                            <a href="/order">
                                <i class="fas fa-shapes"></i>Order</a>
                        </li>
                        <li>
                            <a href="/userdata">
                                <i class="fas fa-user"></i>Users</a>
                        </li>
                        <li>
                            <a href="/">
                                <i class="fas fa-percent"></i>Discount</a>
                        </li>
                        <%if(adminpage.__v===1){%>
                            <li>
                                <a href="/register">
                                    <i class="fa fa-registered"></i>register</a>
                            </li>
                            <li>
                                <a href="/adminpage">
                                    <i class="fa fa-id-card"></i>Admin page</a>
                            </li>
                            <%}%>
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <div class="form-header">
                            </div>
                            <div class="header-button">
                         
                                <div class="account-wrap">
                                    <div class="account-item clearfix js-item-menu">
                                        <div class="image">
                                            <%if(adminpage.__v===1){%>
                                                <img src="/images/icon/manager.jpg" alt="<%=adminpage.username%>" />
                                                <%}else{%>
                                                    <img src="/images/icon/admin.jpg" alt="<%=adminpage.username%>" />
                                                    <%}%>
                                        </div>
                                        <div class="content">
                                            <a class="js-acc-btn" href="#">
                                                <%=adminpage.username%>
                                            </a>
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix">
                                                <div class="image">
                                                    <a href="#">
                                                        <%if(adminpage.__v===1){%>
                                                            <img src="/images/icon/manager.jpg"
                                                                alt="<%=adminpage.username%>" />
                                                            <%}else{%>
                                                                <img src="/images/icon/admin.jpg"
                                                                    alt="<%=adminpage.username%>" />
                                                                <%}%>
                                                    </a>
                                                </div>
                                                <div class="content">
                                                    <h5 class="name">
                                                        <a href="#">
                                                            <%=adminpage.username%>
                                                        </a>
                                                    </h5>
                                                    <span class="email">
                                                        <%=adminpage.email%>
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="account-dropdown__body">
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-account"></i>Account</a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-settings"></i>Setting</a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-money-box"></i>Billing</a>
                                                </div>
                                            </div>
                                            <div class="account-dropdown__footer">
                                                <a href="/logout">
                                                    <i class="zmdi zmdi-power"></i>Logout</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>