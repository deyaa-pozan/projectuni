<%- include('partials/header'); -%>
    

<!-- USER DATA-->
<div class="user-data m-b-30">
    <h3 class="title-3 m-b-30">
        <i class="zmdi zmdi-account-calendar"></i>admin data</h3>
    
    <div class="table-responsive table-data">
        
        <table id="mytable" class="table table-bordred table-striped">
            <thead>
                <tr>
                    <td>
                        <label class="au-checkbox">
                            <input type="checkbox">
                            <span class="au-checkmark"></span>
                        </label>
                    </td>
                    <td>Email</td>
                    <td>role</td>
                    <td>type</td>
                    <td></td>
                </tr>
            
            </thead>

            <tbody>

                <% for (var i =0; i < count1;  i++ ) { %>
                    <tr>
                        <td>
                            <label class="au-checkbox">
                                <input type="checkbox">
                                <span class="au-checkmark"></span>
                            </label>
                        </td>
                        <td>
                            <div class="table-data__info">
                                
                                
                                    <a href="https://www.gmail.com"><%=found1[i].username%></a>
                                
                            </div>
                        </td>
                        <td>
                            <% if(found1[i].__v === 1){ %>
                                <span class="role admin">Manager</span>
                              <% } else{ %>  
                                <span class="role user">admin</span>
                             <% } %>
                            
                        </td>
                        <td>
                            <div class="rs-select2--trans rs-select2--sm">
                                <select class="js-select2" name="property">
                                    <option selected="selected">Full Control</option>
                                    <option value="">Post</option>
                                    <option value="">Watch</option>
                                </select>
                                <div class="dropDownSelect2"></div>
                            </div>
                        </td>
                   
                    <td><p data-placement="top" data-toggle="tooltip" title="Delete"><a href = "/deleteadmin/<%=found1[i]._id %>"><button  class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" ><span class="glyphicon glyphicon-trash"></span></button></a></p></td>

                <% } %>
                    
                    
                 <tr>
            </tbody>
        </table>
    
    </div>
</div>


<!-- END USER DATA-->
        </div>
        <!-- END PAGE CONTAINER-->

    </div>

    <!-- Jquery JS-->
    <script src="vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="vendor/slick/slick.min.js">
    </script>
    <script src="vendor/wow/wow.min.js"></script>
    <script src="vendor/animsition/animsition.min.js"></script>
    <script src="vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="vendor/circle-progress/circle-progress.min.js"></script>
    <script src="vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="vendor/select2/select2.min.js">
